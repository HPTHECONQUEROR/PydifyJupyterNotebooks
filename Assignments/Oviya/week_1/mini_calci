choice=input()
if choice=="a":
    n1=int(input("enter first no:"))
    n2=int(input("enter second no:"))
    print(n1+n2)
if choice=="s":
    n1=int(input("enter first no:"))
    n2=int(input("enter second no:"))
    print(n1-n2)
if choice=="m":
    n1=int(input("enter first no:"))
    n2=int(input("enter second no:"))
    print(n1*n2)
if choice=="d":
    n1=int(input("enter first no:"))
    n2=int(input("enter second no:"))
    print(n1/n2)



    
    